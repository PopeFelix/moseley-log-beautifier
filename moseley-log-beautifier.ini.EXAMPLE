; configuration file for moseley-log-beautifier.pl

; Location of the channel definitions file
channels_file=channels.ini
; Location of transmitter log files as generated by CommServer
transmitter_log_dir=Z:
; Output field order, in terms of channels as specified in channel definitions file
field_order=T33 T34 T41 T48 T32 S1
; Path to output printer. Ignored if print_with_word is set
printer_path=//153.91.87.132/HP DeskJet 712C
; If this is true, then MS Word will be used to generate output and output sent to the default printer on the host running this script
print_with_word=1

header_file=header.txt
